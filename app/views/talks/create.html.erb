<div class="container_12">
  <div class="overflow p3 pad3">
    <h3>Talk Submitted</h3>
  </div>
  <ul class="list4">
    <li>Title</li>
    <li><%= @talk.title %></li>
    <li>Abstract</li>
    <li><%= @talk.abstract %></li>
    <li>Talk Type</li>
    <li><%= @talk.talk_type %></li>
    <li>Talk Length</li>
    <li><%= @talk.talk_length %></li>
    <li>Video Approval</li>
    <li><%= @talk.video_approval %></li>
    <li>Comments</li>
    <li><%= @talk.comments %></li>
    <li>Prereqs</li>
    <li><%= @talk.prereqs %></li>
    <li>Audio/Visual Requirements</li>
    <li><%= @talk.av_requirement %></li>
    <li>Tags</li>
    <li><%= @talk.tag_list %></li>
  </ul>
  <div class="overflow p3">
    <h3>Speaker Info</h3>
  </div>
  <ul class="list4">
    <li>Name</li>
    <li><%= @talk.speakers[0].first_name + " " +  @talk.speakers[0].last_name %></li>
    <li>Email</li>
    <li><%= @talk.speakers[0].email %></li>
    <li>Bio</li>
    <li><%= @talk.speakers[0].bio %></li>
    <li>Phone</li>
    <li><%= @talk.speakers[0].phone %></li>
    <li>City</li>
    <li><%= @talk.speakers[0].city %></li>
    <li>State</li>
    <li><%= @talk.speakers[0].state %></li>
    <li>Country</li>
    <li><%= @talk.speakers[0].country %></li>
  </ul>
</div>
<div class="push"></div>
